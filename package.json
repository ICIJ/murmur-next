{
  "name": "@icij/murmur-next",
  "version": "4.7.1",
  "private": false,
  "description": "Murmur is ICIJ's Design System for Bootstrap 5 and Vue.js",
  "author": "promera@icij.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/ICIJ/murmur-next"
  },
  "bugs": {
    "url": "https://github.com/ICIJ/murmur-next/issues"
  },
  "license": "MIT",
  "scripts": {
    "dev": "yarn storybook",
    "build:lib": "vite build",
    "lint": "eslint",
    "lint:modified": "yarn lint $(git diff --name-only HEAD | xargs)",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write lib/ stories/ .storybook/ tests/",
    "storybook": "storybook dev -p 6006 --no-open",
    "serve": "yarn storybook",
    "build:storybook": "storybook build",
    "test": "vitest",
    "test:unit": "vitest run",
    "prepublish": "yarn build:lib",
    "predeploy": "yarn build:storybook"
  },
  "main": "./dist/lib/murmur.umd.cjs",
  "module": "./dist/lib/murmur.js",
  "files": [
    "lib/**/*",
    "dist/lib/**/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/lib/murmur.js",
      "require": "./dist/lib/murmur.umd.cjs"
    },
    "./lib/": "./lib/",
    "./dist/": "./dist/"
  },
  "type": "module",
  "dependencies": {
    "@storybook/preset-scss": "^1.0.3",
    "@types/topojson": "^3.2.6",
    "bootstrap": "^5.3.8",
    "bootstrap-vue-next": "0.42.0",
    "canvas": "^3.2.0",
    "clipboard": "^2.0.11",
    "d3": "^7.9.0",
    "d3-geo-projection": "^4.0.0",
    "jsonp": "^0.2.1",
    "lodash": "^4.17.21",
    "mutationobserver-shim": "^0.3.7",
    "querystring-es3": "^0.2.1",
    "resize-observer-polyfill": "^1.5.1",
    "topojson": "^3.0.2",
    "vue-input-autowidth": "^2.2.1",
    "vue-virtual-scroller": "^2.0.0-beta.8"
  },
  "peerDependencies": {
    "vue": "^3.5.18",
    "vue-i18n": "^9.13.1"
  },
  "devDependencies": {
    "@floating-ui/vue": "^1.1.9",
    "@icij/eslint-config": "^3.0.3",
    "@iconify-json/ph": "^1.2.2",
    "@intlify/unplugin-vue-i18n": "^11.0.0",
    "@rushstack/eslint-patch": "^1.15.0",
    "@storybook/addon-docs": "^10.0.0",
    "@storybook/addon-links": "^10.0.0",
    "@storybook/addon-themes": "^10.0.0",
    "@storybook/vue3-vite": "^10.0.0",
    "@types/d3": "^7.4.3",
    "@types/d3-geo": "^3.1.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jsonp": "^0.2.3",
    "@types/lodash": "^4.17.21",
    "@types/node": "^24.0.14",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^14.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-storybook": "^10.0.0",
    "globals": "^17.0.0",
    "jsdom": "^26.1.0",
    "msw": "^2.12.7",
    "msw-storybook-addon": "^2.0.6",
    "prettier": "^3.7.4",
    "sass": "^1.97.1",
    "storybook": "^10.0.0",
    "typescript": "^5.9.3",
    "unplugin-auto-import": "^20.0.0",
    "unplugin-icons": "^22.5.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "vue": "^3.5.26",
    "vue-component-meta": "^3.2.1",
    "vue-headroom": "github:caro3801/vue-headroom",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.6.4"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "none"
  }
}
