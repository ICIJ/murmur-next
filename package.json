{
  "name": "@icij/murmur-next",
  "version": "4.5.1",
  "private": false,
  "description": "Murmur is ICIJ's Design System for Bootstrap 5 and Vue.js",
  "author": "promera@icij.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/ICIJ/murmur-next"
  },
  "bugs": {
    "url": "https://github.com/ICIJ/murmur-next/issues"
  },
  "license": "MIT",
  "scripts": {
    "dev": "yarn storybook",
    "build:lib": "vite build",
    "lint": "eslint --ignore-pattern storybook-static .",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write lib/ stories/ .storybook/ tests/",
    "storybook": "storybook dev -p 6006 --no-open",
    "serve": "yarn storybook",
    "build:storybook": "storybook build",
    "test": "vitest",
    "test:unit": "vitest run",
    "prepublish": "yarn build:lib",
    "predeploy": "yarn build:storybook"
  },
  "main": "./dist/lib/murmur.umd.cjs",
  "module": "./dist/lib/murmur.js",
  "files": [
    "lib/**/*",
    "dist/lib/**/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/lib/murmur.js",
      "require": "./dist/lib/murmur.umd.cjs"
    },
    "./lib/": "./lib/",
    "./dist/": "./dist/"
  },
  "type": "module",
  "dependencies": {
    "@phosphor-icons/vue": "^2.2.1",
    "@storybook/preset-scss": "^1.0.3",
    "@types/topojson": "^3.2.6",
    "bootstrap": "^5.3.7",
    "bootstrap-vue-next": "0.30.3",
    "canvas": "^3.1.2",
    "clipboard": "^2.0.11",
    "d3": "^7.9.0",
    "d3-geo-projection": "^4.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "jsonp": "^0.2.1",
    "lodash": "^4.17.21",
    "mutationobserver-shim": "^0.3.7",
    "querystring-es3": "^0.2.1",
    "resize-observer-polyfill": "^1.5.1",
    "topojson": "^3.0.2",
    "vue-input-autowidth": "^2.2.1",
    "vue-virtual-scroller": "^2.0.0-beta.8"
  },
  "peerDependencies": {
    "vue": "^3.4.38",
    "vue-i18n": "^9.13.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@floating-ui/vue": "^1.1.6",
    "@icij/eslint-config": "^2.0.0",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@rushstack/eslint-patch": "^1.12.0",
    "@storybook/addon-docs": "^9.0.15",
    "@storybook/addon-links": "^9.0.15",
    "@storybook/addon-themes": "^9.0.15",
    "@storybook/vue3-vite": "^9.0.15",
    "@types/d3": "^7.4.3",
    "@types/d3-geo": "^3.1.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jsonp": "^0.2.3",
    "@types/lodash": "^4.17.20",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^13.5.0",
    "eslint": "^9.30.1",
    "eslint-plugin-storybook": "^9.0.15",
    "eslint-plugin-vue": "^10.3.0",
    "jsdom": "^26.1.0",
    "msw": "^2.7.0",
    "msw-storybook-addon": "^2.0.5",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "storybook": "^9.0.15",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^7.0.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "vue": "^3.5.17",
    "vue-component-meta": "^3.0.1",
    "vue-headroom": "github:caro3801/vue-headroom",
    "vue-i18n": "^11.1.9",
    "vue-router": "^4.5.1"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "none"
  }
}
